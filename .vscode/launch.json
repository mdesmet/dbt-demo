{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/venv/lib/python3.9/site-packages/dbt_osmosis/main.py",
            "args": [
                "workbench"
            ],
            "justMyCode": false,
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: SubProcess",
            "type": "python",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": false,
            "logToFile": true
        },
        {
            "name": "Dynamic Python: File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "justMyCode": false
        }
    ]
}